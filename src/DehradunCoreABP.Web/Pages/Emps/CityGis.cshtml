        }

            #equal:hover, #notequal:hover, #like:hover, #gt:hover, #gtequal:hover, #perc:hover, #lt:hover, #not:hover, #And:hover, #Or:hover, #ltequal:hover {
                background-color: #0082e6;
                color: white;
            }





        .bottombtn {
            visibility: visible;
            top: 98%;
            left: 40%;
            position: absolute;
        }

        #uparrowbtn {
            position: absolute;
            top: 92%;
            left: 40%;
            visibility: hidden;
            /* margin-left: 49%; */
        }

            #uparrowbtn.active {
                top: 85%;
